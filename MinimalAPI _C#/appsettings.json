{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Kestrel": {
        "Limits": {
            "MaxRequestBodySize": 104857600
        }
    },
    "FormOptions": {
        "MultipartBodyLengthLimit": 104857600
    }

}


